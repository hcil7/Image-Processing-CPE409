   "source": [
    "import cv2\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "c1af3881",
   "metadata": {},
   "outputs": [],
   "source": [
    "image=cv2.imread('view.jpg')\n",
    "kernel1=np.ones((5,5),np.float32)/30\n",
    "img=cv2.filter2D(src=image,ddepth=-1,kernel=kernel1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "aef026e5",
   "metadata": {},
   "outputs": [],
   "source": [
    "cv2.imshow('Original',image)\n",
    "cv2.imshow('Kernel Blur',img)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  },
