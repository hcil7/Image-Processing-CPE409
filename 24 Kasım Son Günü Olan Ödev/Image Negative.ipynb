    "import cv2\n",
    "import numpy as np\n",
    "# Load the image\n",
    "img = cv2.imread('view.jpg')\n",
    "# Check the datatype of the image\n",
    "print(img.dtype)\n",
    "# Subtract the img from max value(calculated from dtype)\n",
    "img_neg = 255 - img\n",
    "# Show the image\n",
    "cv2.imshow('negative',img_neg)\n",
    "cv2.waitKey(0)"
   ]
